<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="teamBox" maxWidth="1.7976931348623157E308" prefHeight="400.0"
      prefWidth="200.0" xmlns="http://javafx.com/javafx/11.0.14-internal"
      fx:controller="de.olivergeisel.kegelplay.gui.TeamCompleteViewController">
    <children>
    <VBox prefHeight="200.0" prefWidth="100.0">
        <Label fx:id="teamLabel" alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308"
               maxWidth="1.7976931348623157E308" text="Team " VBox.vgrow="ALWAYS" minWidth="150">
            <font>
                <Font size="30.0"/>
            </font>
        </Label>
        <VBox fx:id="players" prefHeight="200.0" prefWidth="100.0">
            <children>
                <fx:include source="player-team-overview.fxml"/>
                <fx:include source="player-team-overview.fxml"/>
                <fx:include source="player-team-overview.fxml"/>
                <fx:include source="player-team-overview.fxml"/>
                <fx:include source="player-team-overview.fxml"/>
                <fx:include source="player-team-overview.fxml"/>
            </children>
        </VBox>
    </VBox>

    </children>
    <opaqueInsets>
        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
    </opaqueInsets>
    <padding>
        <Insets left="10.0" right="10.0"/>
    </padding>
</VBox>
